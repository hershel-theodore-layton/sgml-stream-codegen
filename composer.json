{
  "name": "hershel-theodore-layton/sgml-stream-codegen",
  "description": "Build your own custom native sgml-stream tags from a json definition.",
  "license": "MIT",
  "require": {
    "hhvm": "^4.102",
    "facebook/hack-codegen": "^4.4",
    "hhvm/type-assert": "^4.2"
  },
  "require-dev": {
    "hhvm/hhvm-autoload": "^3.2",
    "hhvm/hhast": "^4.102",
    "hhvm/hacktest": "^2.3",
    "facebook/fbexpect": "^2.8"
  },
  "scripts": {
    "post-install-cmd": [
      "rm vendor/autoload.php vendor/composer/*.php"
    ],
    "post-update-cmd": [
      "rm vendor/autoload.php vendor/composer/*.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "hhvm/hhvm-autoload": true
    }
  }
}
