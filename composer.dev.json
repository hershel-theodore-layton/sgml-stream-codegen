{
  "name": "hershel-theodore-layton/sgml-stream-codegen",
  "description": "Build your own custom native sgml-stream tags from a json definition.",
  "license": "MIT",
  "require": {
    "hhvm": "^4.102"
  },
  "require-dev": {
    "hershel-theodore-layton/static-type-assertion-code-generator": "^1",
    "hershel-theodore-layton/portable-hack-ast-linters-server": "<1"
  },
  "scripts": {
    "post-install-cmd": [".github/workflows/prepare-environment.sh"],
    "post-update-cmd": [".github/workflows/prepare-environment.sh"]
  },
  "config": {
    "allow-plugins": {
      "hhvm/hhvm-autoload": true
    }
  }
}
